<%- include('header') -%>


<div class="container mt-5 mb-5">
  <h1>Student Details</h1>
  <div class="row">
    <div class="col-md-6">
      <ul class="list-group">
        <% for (const key in studentDetails) { %>
          <li class="list-group-item">
            <strong><%= key %>:</strong> <%= studentDetails[key] %>
          </li>
        <% } %>
      </ul>
    </div>
  </div>
</div>







<%- include('footer') -%>